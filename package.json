{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "Solid js yoga circle app with firebase",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && pnpm build-storybook --output-dir dist/storybook",
    "serve": "vite preview",
    "test": "vitest run",
    "test-watch": "vitest",
    "format-fix": "prettier --write \"**/*.{js,jsx,ts,tsx,scss,css,md,json,svg}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "pnpm run lint --fix",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "devDependencies": {
    "@solidjs/testing-library": "^0.7.1",
    "@storybook/addon-essentials": "7.4.2",
    "@storybook/addon-interactions": "7.4.2",
    "@storybook/addon-links": "7.4.2",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "7.4.2",
    "@storybook/testing-library": "0.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-solid": "^0.12.1",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.28",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.4.2",
    "storybook-solidjs": "1.0.0-beta.2",
    "storybook-solidjs-vite": "1.0.0-beta.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.34.2"
  },
  "dependencies": {
    "firebase": "^10.2.0",
    "solid-js": "^1.7.11"
  }
}
