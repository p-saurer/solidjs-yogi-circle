{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "Solid js yoga circle app with firebase",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && pnpm build-storybook --output-dir dist/storybook",
    "serve": "vite preview",
    "format-fix": "prettier --write \"**/*.{js,jsx,ts,tsx,scss,css,md,json,svg}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "pnpm run lint --fix",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "license": "MIT",
  "devDependencies": {
    "@solidjs/testing-library": "^0.6.1",
    "@storybook/addon-essentials": "7.0.0-rc.4",
    "@storybook/addon-interactions": "7.0.0-rc.4",
    "@storybook/addon-links": "7.0.0-rc.4",
    "@storybook/addon-styling": "^0.3.1",
    "@storybook/blocks": "7.0.0-rc.4",
    "@storybook/testing-library": "0.0.14-next.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-solid": "^0.9.4",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.0.0-rc.4",
    "storybook-solidjs": "1.0.0-beta.1",
    "storybook-solidjs-vite": "1.0.0-beta.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vite-plugin-solid": "^2.6.1"
  },
  "dependencies": {
    "firebase": "^9.18.0",
    "solid-js": "^1.6.15"
  }
}
